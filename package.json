{
  "private": true,
  "name": "stsearch",
  "version": "0.0.0",
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "lint": "eslint .",
    "build": "npm run build:dev && npm run build:prod",
    "build:dev": "webpack --display-reasons",
    "build:prod": "webpack --display-reasons --mode=production",
    "watch": "npm run watch:dev",
    "watch:dev": "webpack --display-reasons --watch",
    "watch:prod": "webpack --display-reasons --watch --mode=production",
    "serve": "npm run serve:dev",
    "serve:dev": "webpack-dev-server",
    "serve:prod": "webpack-dev-server --mode=production"
  },
  "dependencies": {
    "@appnest/lit-translate": "~1.1.10",
    "@fortawesome/fontawesome-free": "~5.7.2",
    "@fortawesome/fontawesome-svg-core": "~1.2.15",
    "@fortawesome/free-brands-svg-icons": "~5.7.2",
    "@fortawesome/free-regular-svg-icons": "~5.7.2",
    "@fortawesome/free-solid-svg-icons": "~5.7.2",
    "@yaireo/tagify": "~2.13.0",
    "bootstrap": "~4.3.1",
    "jquery": "~3.3.1",
    "lit-element": "~2.0.1",
    "lit-html": "~1.0.0",
    "lodash": "~4.17.11",
    "noty": "~v3.2.0-beta",
    "popper.js": "~1.14.7",
    "unfetch": "~4.1.0"
  },
  "devDependencies": {
    "@babel/core": "~7.3.4",
    "@babel/plugin-proposal-class-properties": "~7.3.4",
    "@babel/preset-env": "~7.3.4",
    "autoprefixer": "~9.4.9",
    "babel-loader": "~8.0.5",
    "chai": "~4.2.0",
    "clean-webpack-plugin": "~1.0.1",
    "copy-webpack-plugin": "~5.0.0",
    "css-loader": "~2.1.0",
    "cssnano": "~4.1.10",
    "eslint": "~5.15.0",
    "eslint-config-xo": "~0.26.0",
    "eslint-config-zant": "~2.1.0",
    "eslint-plugin-import": "~2.16.0",
    "eslint-plugin-unicorn": "~7.1.0",
    "exports-loader": "~0.7.0",
    "html-webpack-plugin": "~3.2.0",
    "mini-css-extract-plugin": "~0.5.0",
    "mocha": "~6.0.2",
    "node-sass": "~4.11.0",
    "postcss-loader": "~3.0.0",
    "sass-loader": "~7.1.0",
    "svg-url-loader": "~2.3.2",
    "svgo-loader": "~2.2.0",
    "terser-webpack-plugin": "~1.2.3",
    "webpack": "~4.29.6",
    "webpack-cli": "~3.2.3",
    "webpack-dev-server": "~3.2.1"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "browserslist": [
    "Firefox ESR",
    "Last 2 Chrome versions",
    "Last 2 Edge versions",
    "Last 2 Firefox versions",
    "Last 2 iOS versions",
    "Last 2 Opera versions",
    "Last 2 Safari versions"
  ]
}
