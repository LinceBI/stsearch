{
  "private": true,
  "name": "stsearch",
  "version": "1.0.0",
  "maven": {
    "group": "com.stratebi.pentaho",
    "repository": {
      "snapshot": {
        "id": "stratebi-snapshot",
        "url": "https://repo.stratebi.com/repository/stratebi-mvn-snapshot"
      },
      "release": {
        "id": "stratebi-releases",
        "url": "https://repo.stratebi.com/repository/stratebi-mvn-releases"
      }
    }
  },
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "lint": "eslint --fix ./",
    "build": "npm run build:dev",
    "build:dev": "webpack --display-reasons",
    "build:prod": "webpack --display-reasons --mode=production",
    "watch": "npm run watch:dev",
    "watch:dev": "webpack --display-reasons --watch",
    "watch:prod": "webpack --display-reasons --watch --mode=production",
    "serve": "npm run serve:dev",
    "serve:dev": "webpack-dev-server",
    "serve:prod": "webpack-dev-server --mode=production",
    "deploy": "npm run deploy:dev",
    "deploy:dev": "node ./scripts/mvn-deploy.js",
    "deploy:prod": "NODE_ENV=production node ./scripts/mvn-deploy.js"
  },
  "dependencies": {
    "@appnest/lit-translate": "~1.1.14",
    "@fortawesome/fontawesome-free": "~5.8.1",
    "@fortawesome/fontawesome-svg-core": "~1.2.17",
    "@fortawesome/free-brands-svg-icons": "~5.8.1",
    "@fortawesome/free-regular-svg-icons": "~5.8.1",
    "@fortawesome/free-solid-svg-icons": "~5.8.1",
    "@yaireo/tagify": "~2.16.0",
    "bootstrap": "~4.3.1",
    "bs-custom-file-input": "~1.3.2",
    "compressorjs": "~1.0.5",
    "jquery": "~3.4.0",
    "lit-element": "~2.1.0",
    "lit-html": "~1.0.0",
    "lodash": "~4.17.11",
    "noty": "~v3.2.0-beta",
    "popper.js": "~1.15.0",
    "unfetch": "~4.1.0"
  },
  "devDependencies": {
    "@babel/core": "~7.4.3",
    "@babel/plugin-proposal-class-properties": "~7.4.0",
    "@babel/preset-env": "~7.4.3",
    "archiver": "~3.0.0",
    "autoprefixer": "~9.5.1",
    "babel-eslint": "~10.0.1",
    "babel-loader": "~8.0.5",
    "chai": "~4.2.0",
    "clean-webpack-plugin": "~2.0.1",
    "copy-webpack-plugin": "~5.0.2",
    "css-loader": "~2.1.1",
    "cssnano": "~4.1.10",
    "eslint": "~5.16.0",
    "eslint-config-airbnb-base": "~13.1.0",
    "eslint-config-prettier": "~4.1.0",
    "eslint-plugin-import": "~2.16.0",
    "exports-loader": "~0.7.0",
    "html-webpack-plugin": "~3.2.0",
    "mini-css-extract-plugin": "~0.6.0",
    "mocha": "~6.1.2",
    "node-sass": "~4.11.0",
    "postcss-loader": "~3.0.0",
    "prettier": "~1.16.4",
    "sass-loader": "~7.1.0",
    "svg-url-loader": "~2.3.2",
    "svgo-loader": "~2.2.0",
    "terser-webpack-plugin": "~1.2.3",
    "webpack": "~4.29.6",
    "webpack-cli": "~3.3.0",
    "webpack-dev-server": "~3.3.1"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "browserslist": [
    "Firefox ESR",
    "Last 2 Chrome versions",
    "Last 2 Edge versions",
    "Last 2 Firefox versions",
    "Last 2 iOS versions",
    "Last 2 Opera versions",
    "Last 2 Safari versions"
  ]
}
