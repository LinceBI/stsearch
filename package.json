{
  "private": true,
  "name": "stsearch",
  "version": "1.4.0",
  "license": "GPL-3.0-or-later",
  "maven": {
    "group": "com.stratebi.lincebi",
    "repository": {
      "snapshot": {
        "id": "lincebi-snapshots",
        "url": "https://repo.stratebi.com/repository/lincebi-mvn-snapshots"
      },
      "release": {
        "id": "lincebi-releases",
        "url": "https://repo.stratebi.com/repository/lincebi-mvn-releases"
      }
    }
  },
  "scripts": {
    "lint": "eslint --fix ./",
    "build": "npm run build:dev",
    "build:dev": "webpack --display-reasons",
    "build:prod": "webpack --display-reasons --mode=production",
    "watch": "npm run watch:dev",
    "watch:dev": "webpack --display-reasons --watch",
    "watch:prod": "webpack --display-reasons --watch --mode=production",
    "serve": "npm run serve:dev",
    "serve:dev": "webpack-dev-server",
    "serve:prod": "webpack-dev-server --mode=production",
    "deploy": "npm run deploy:dev",
    "deploy:dev": "node ./scripts/mvn-deploy.js",
    "deploy:prod": "NODE_ENV=production node ./scripts/mvn-deploy.js"
  },
  "dependencies": {
    "@appnest/lit-translate": "~1.1.18",
    "@babel/runtime": "~7.8.7",
    "@fortawesome/fontawesome-free": "~5.12.1",
    "@fortawesome/fontawesome-svg-core": "~1.2.27",
    "@fortawesome/free-brands-svg-icons": "~5.12.1",
    "@fortawesome/free-regular-svg-icons": "~5.12.1",
    "@fortawesome/free-solid-svg-icons": "~5.12.1",
    "@yaireo/tagify": "~3.6.2",
    "bootstrap": "~4.4.1",
    "bs-custom-file-input": "~1.3.4",
    "compressorjs": "~1.0.6",
    "jquery": "~3.4.1",
    "lit-element": "~2.2.1",
    "lit-html": "~1.1.2",
    "lodash": "~4.17.15",
    "noty": "~v3.2.0-beta",
    "popper.js": "~1.16.1",
    "unfetch": "~4.1.0"
  },
  "devDependencies": {
    "@babel/core": "~7.8.7",
    "@babel/plugin-syntax-dynamic-import": "~7.8.3",
    "@babel/plugin-transform-runtime": "~7.8.3",
    "@babel/preset-env": "~7.8.7",
    "archiver": "~3.1.1",
    "autoprefixer": "~9.7.4",
    "babel-eslint": "~10.1.0",
    "babel-loader": "~8.0.6",
    "chai": "~4.2.0",
    "clean-webpack-plugin": "~3.0.0",
    "copy-webpack-plugin": "~5.1.1",
    "css-loader": "~3.4.2",
    "cssnano": "~4.1.10",
    "eslint": "~6.8.0",
    "eslint-config-airbnb-base": "~14.1.0",
    "eslint-config-hectorm": "~1.0.4",
    "eslint-plugin-import": "~2.20.1",
    "exports-loader": "~0.7.0",
    "fibers": "~4.0.2",
    "html-webpack-plugin": "~3.2.0",
    "mini-css-extract-plugin": "~0.9.0",
    "mocha": "~7.1.0",
    "postcss-loader": "~3.0.0",
    "sass": "~1.26.3",
    "sass-loader": "~8.0.2",
    "svg-url-loader": "~4.0.0",
    "svgo-loader": "~2.2.1",
    "terser-webpack-plugin": "~2.3.5",
    "webpack": "~4.42.0",
    "webpack-cli": "~3.3.11",
    "webpack-dev-server": "~3.10.3"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-syntax-dynamic-import",
      "@babel/plugin-transform-runtime"
    ]
  },
  "browserslist": [
    "Last 2 versions and not dead",
    "Firefox ESR"
  ]
}
